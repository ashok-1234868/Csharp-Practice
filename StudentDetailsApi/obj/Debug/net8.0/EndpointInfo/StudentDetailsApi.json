{
  "openapi": "3.0.1",
  "info": {
    "title": "StudentDetailsApi",
    "version": "1.0"
  },
  "paths": {
    "/api/Student/add": {
      "post": {
        "tags": [
          "Student"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StudentDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/StudentDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/StudentDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Student/getAll": {
      "get": {
        "tags": [
          "Student"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AddressDto": {
        "required": [
          "addressType",
          "city",
          "pincode",
          "state",
          "street"
        ],
        "type": "object",
        "properties": {
          "addressType": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "street": {
            "maxLength": 250,
            "minLength": 1,
            "type": "string"
          },
          "city": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "pincode": {
            "minLength": 1,
            "type": "string"
          },
          "state": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "StudentDto": {
        "required": [
          "address",
          "dob",
          "email",
          "gender",
          "mobileNo",
          "name",
          "rollNo"
        ],
        "type": "object",
        "properties": {
          "rollNo": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "gender": {
            "minLength": 1,
            "type": "string"
          },
          "dob": {
            "type": "string",
            "format": "date"
          },
          "email": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "mobileNo": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "address": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AddressDto"
            }
          }
        },
        "additionalProperties": false
      }
    }
  }
}